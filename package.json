{
  "name": "quicktab",
  "version": "0.0.2",
  "description": "multi iframe tab plugin",
  "main": "dist/js/quicktab.min.js",
  "scripts": {
    "css:lint": "stylelint --config stylelint.config.mjs src/**/*.scss --cache --cache-location .cache/.stylelintcache",
    "css:lint:fix": "stylelint --config stylelint.config.mjs src/**/*.scss --fix",
    "css:format": "prettier --config prettier.config.mjs  src/**/*.scss --check --cache --cache-location .cache/.prettiercache",
    "css:format:fix": "prettier --config prettier.config.mjs src/**/*.scss --write",
    "css:lint-and-format": "npm run css:lint && npm run css:format",
    "css:fix": "run-s css:*:fix",
    "css:compile": "sass --style expanded --source-map --embed-sources --no-error-css src/:dist/css/",
    "css:prefix": "postcss --config postcss.config.mjs --replace \"dist/css/*.css\"  \"!dist/css/*.min.css\"",
    "css:min": "cleancss -O1  --format breakWith=lf --with-rebase  --source-map --source-map-inline-sources --output dist/css/ --batch --batch-suffix \".min\" \"dist/css/*.css\" \"!dist/css/*.min.css\"",
    "css:banner": "node build/banner-builder.mjs dist/css/*.css",
    "css": "run-s \"css:!(fix|lint-and-format)\"",
    "js:lint": "cross-env ESLINT_USE_FLAT_CONFIG=true eslint --config eslint.config.mjs src/**/*.js --cache --cache-location .cache/.eslintcache --report-unused-disable-directives",
    "js:lint:fix": "cross-env ESLINT_USE_FLAT_CONFIG=true eslint --config eslint.config.mjs src/**/*.js  --fix",
    "js:format": "prettier --config prettier.config.mjs  src/**/*.js --check --cache --cache-location .cache/.prettiercache",
    "js:format:fix": "prettier --config prettier.config.mjs src/**/*.js --write",
    "js:lint-and-format": "run-p \"js:*(lint|format)\"",
    "js:fix": "run-s js:*:fix",
    "js:base": "rollup --config rollup.config.mjs",
    "js:min": "cross-env NODE_ENV=production rollup --config rollup.config.mjs",
    "js:compile": "run-p \"js:*(base|min)\"",
    "js": "run-s \"js:*(lint-and-format|compile)\"",
    "event:build": "node build/event-helper-build.mjs",
    "watch:css": "nodemon --watch src/**/*.scss --ext scss --exec \"run-s css\"",
    "watch:js": "nodemon --watch src/**/*.js --ext js --exec \"run-s js\"",
    "watch": "run-p watch-*",
    "lint": "run-p --aggregate-output -c *:lint-and-format",
    "fix": "run-p --aggregate-output  -c *:fix",
    "clean": "shx rm -rf dist",
    "prepare": "husky install",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs",
    "demo": "shx mkdir -p docs/public/Quicktab/ && shx cp -r dist/ demo/ docs/public/Quicktab/",
    "dist": "run-s clean  && run-p css js && run-s demo docs:build",
    "commitlint": "commitlint --config commitlint.config.mjs -e -V"
  },
  "repository": {
    "type": "git",
    "url": "https://gitee.com/ajiho/quicktab.git"
  },
  "keywords": [
    "tab",
    "iframe",
    "Multiple tabs"
  ],
  "homepage": "https://gitee.com/ajiho/quicktab",
  "author": "ajiho",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.23.7",
    "@babel/preset-env": "^7.23.7",
    "@commitlint/cli": "^18.4.3",
    "@commitlint/config-conventional": "^18.4.3",
    "@eslint/js": "^8.56.0",
    "@floating-ui/dom": "^1.5.4",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-multi-entry": "^6.0.1",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "autoprefixer": "^10.4.16",
    "chalk": "^5.3.0",
    "clean-css-cli": "^5.6.3",
    "core-js": "^3.35.0",
    "cross-env": "^7.0.3",
    "del": "^7.1.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "execa": "^8.0.1",
    "fs-extra": "^11.2.0",
    "glob": "^10.3.10",
    "globals": "^13.24.0",
    "husky": "^8.0.0",
    "node-css-mqpacker": "^9.0.0",
    "nodemon": "^3.0.2",
    "npm-run-all2": "^6.1.1",
    "postcss": "^8.4.32",
    "postcss-cli": "^11.0.0",
    "prettier": "^3.2.3",
    "rollup": "^4.9.6",
    "rollup-plugin-strip-banner": "^3.1.0",
    "sass": "^1.69.7",
    "shx": "^0.3.4",
    "simplebar": "^6.2.5",
    "stylelint": "^16.2.0",
    "stylelint-config-recess-order": "^4.4.0",
    "stylelint-config-standard-scss": "^12.0.0",
    "superstruct": "^1.0.3",
    "vitepress": "^1.0.0-rc.40",
    "vitepress-plugin-tabs": "^0.5.0"
  }
}